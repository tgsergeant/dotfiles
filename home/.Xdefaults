Xcursor.theme:    Neutral

URxvt*font: xft:DejaVu Sans Mono:pixelsize=13:antialias=true:hinting=true
URxvt*boldFont: xft:DejaVu Sans Mono:pixelsize=13:antialias=true:hinting=true

URxvt*scrollBar_right:true
URxvt*scrollstyle:	plain
URxvt.letterSpace: -1


!https://github.com/chriskempson/tomorrow-theme/blob/master/Xdefaults/XresourceTomorrowNight
!#define t_background #1d1f21
!#define t_current_line #282a2e
!#define t_selection         #373b41
!#define t_foreground         #c5c8c6
!#define t_comment                 #969896
!#define t_red                         #cc6666
!#define t_orange                 #de935f
!#define t_yellow         #f0c674
!#define t_green                 #b5bd68
!#define t_aqua                 #8abeb7
!#define t_blue                 #81a2be
!#define t_purple         #b294bb


*.foreground: #c5c8c6
*.background: #1d1f21
*.cursorColor: #aeafad

! Black / Grey
*.color0: #000000
*.color8: #666666

! Red / Bright Red
*.color1: #cc6666
*.color9: #ff3334

! Green + Bright Green
*.color2: #b5bd68
*.color10: #9ec400

! Yellow (Orange) + Bright Yellow (Yellow)
*.color3: #de935f
*.color11: #f0c674

! Blue + Bright Blue
*.color4: #81a2be
*.color12: #81a2be

! Magenta (Purple) + Bright Magenta
*.color5: #b294bb
*.color13: #b777e0

! Cyan (Aqua) + Bright Cyan
*.color6: #8abeb7
*.color14: #54ced6

! Light Grey (Selection) + White (Current Line)
*.color7: #373b41
*.color15: #282a2e

Xft.autohint: false
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.dpi: 96
Xft.rgba: rgb

